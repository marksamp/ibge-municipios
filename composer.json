{
  "name": "marksamp/ibge-localidades",
  "description": "Biblioteca PHP para consumir APIs do IBGE, com foco inicial em localidades (Estados e Municípios)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Márcio Sampaio",
      "email": "marcio_ce@hotmail.com"
    }
  ],
  "require": {
    "php": ">=8.0.7",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Marksamp\\IbgeLocalidades\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marksamp\\IbgeLocalidades\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "keywords": [
    "ibge",
    "brasil",
    "estados",
    "municipios",
    "localidades",
    "api"
  ]
}